{
  "rpc": {
    "listen": "tcp://127.0.0.1:6004"
  },

  "server": {
    "command": "php ../../http/client.php echo pipes",
    "relay": "pipes",
    "relay_timeout": "20s"
  },

  "http": {
    "address": "127.0.0.1:18903",
    "max_request_size": 1024,
    "middleware": [],
    "ssl": {
      "address": ":8893",
      "redirect": false,
      "cert": "fixtures/server.crt",
      "key": "fixtures/server.key",
      "acme": {
        "certificate_authority": "letsencrypt",
        "dir": "",
        "challenge": "http-01",
        "token": ""
      }
    },

    "pool": {
      "num_workers": 2,
      "max_jobs": 0,
      "allocate_timeout": "60s",
      "destroy_timeout": "60s"
    }
  },

  "logs": {
    "mode": "development",
    "level": "debug"
  }
}