{
    "name": "spiral/roadrunner-grpc",
    "type": "library",
    "description": "High-Performance GRPC server for PHP applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Titov / Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/polyfill-php80": "^1.22",
        "google/protobuf": "^3.7",
        "spiral/roadrunner": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spiral/code-style": "^1.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "symfony/var-dumper": ">=4.4",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Spiral\\RoadRunner\\GRPC\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GPBMetadata\\": "tests/generated/GPBMetadata",
            "Service\\": "tests/generated/Service",

            "Spiral\\RoadRunner\\GRPC\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "phpcs --standard=phpcs.xml",
            "psalm --no-cache",
            "phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
